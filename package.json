{
  "name": "@nethesis/vue-components",
  "type": "module",
  "version": "0.1.0",
  "private": false,
  "keywords": [
    "nethserver",
    "nethesis",
    "vuejs",
    "vue",
    "components"
  ],
  "homepage": "https://github.com/nethesis/vue-components",
  "bugs": "https://github.com/nethesis/vue-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/nethesis/vue-components.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "tailwind.config.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-components.js",
      "require": "./dist/vue-components.umd.cjs",
      "types": "./dist/main.d.ts"
    },
    "./tailwind.config.ts": "./tailwind.config.ts"
  },
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-themes": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/test": "^7.6.3",
    "@storybook/vue3": "^7.6.3",
    "@storybook/vue3-vite": "^7.6.3",
    "@tailwindcss/forms": "^0.5.7",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.3",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-plugin-dts": "^3.6.4",
    "vue-tsc": "^1.8.19"
  }
}
